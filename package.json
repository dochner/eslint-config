{
  "name": "@dochner/eslint-config-monorepo",
  "version": "0.0.1",
  "packageManager": "pnpm@7.23.0",
  "private": false,
  "license": "MIT",
  "author": "dochner <douglasochner@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r build",
    "stub": "pnpm -r stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "pnpm bump && pnpm publish",
    "bump": "pnpm dlx bumpp package.json packages/*/package.json -x 'pnpm changelog' --all",
    "publish": "pnpm -r --filter './packages/*' publish --access public --no-git-checks",
    "type-check": "pnpm -r type-check"
  },
  "devDependencies": {
    "@dochner/eslint-config": "workspace:*",
    "@dochner/eslint-plugin": "workspace:*",
    "@types/react": "^18.0.26",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.31.0",
    "jiti": "^1.16.1",
    "react": "^18.2.0",
    "typescript": "^4.9.4",
    "zx": "^7.1.1"
  },
  "description": "My ESLint config preset",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-import-resolver-node": "^0.3.7",
    "conventional-changelog-eslint": "^3.0.9",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsonc": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-module-utils": "^2.7.4",
    "eslint-plugin-vue": "^9.8.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-yml": "^1.4.0",
    "jsonc-eslint-parser": "^2.1.0",
    "eslint-visitor-keys": "^3.3.0",
    "yaml-eslint-parser": "^1.1.0",
    "eslint-scope": "^5.1.1",
    "vue-eslint-parser": "^9.1.0",
    "eslint-utils": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dochner/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "bugs": {
    "url": "https://github.com/dochner/eslint-config/issues"
  },
  "homepage": "https://github.com/dochner/eslint-config#readme"
}
